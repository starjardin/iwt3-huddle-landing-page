: 100%;\n}\n\nh1, h2, h3, h4, h5, h6 {\n  font-family: \"Poppins\", Arial, Helvetica, sans-serif;\n  font-weight: 700;\n}\n\nbutton {\n  cursor: pointer;\n}","scss\\masthead.scss":"@import \"./variables.scss\";\n@import \"./mixin.scss\";\n\n.masthead {\n  text-align: center;\n  padding-top: 25%;\n  padding-bottom: 15%;\n}\n\n.masthead__heading {\n  font-weight: 700;\n}\n\n.masthead__paragraph {\n  font-weight: 400;\n  margin: 2rem 0 3rem 0;\n}\n\n.masthead__button {\n  @include btn;\n  background-color: $pink;\n  color: #fefefe;\n  padding: .6rem 2.3rem;\n}","scss\\mixin.scss":"@import \"./variables.scss\";\n\n@mixin flex-row {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  align-items: center;\n}\n\n@mixin bg-left {\n  background-position: left center;\n  background-repeat: no-repeat;\n  background-size: contain;\n}\n\n@mixin btn {\n  border-radius: 25px;\n  border: 1.5px solid $pink;\n  text-transform: capitalize;\n}","scss\\variables.scss":"$pink: hsl(322, 100%, 66%);\n$light-pink: hsl(321, 100%, 78%);\n$light-red: hsl(0, 100%, 63%);\n$very-dark-cyan: hsl(192, 100%, 9%);\n$very-pale-blue: hsl(207, 100%, 98%);","scss\\header.scss":"@import \"./mixin.scss\";\n@import \"./variables.scss\";\n\n.header__container {\n  @include flex-row;\n}\n\n.header__heading {\n  background-image: url('../images/icon-messages.svg');\n  @include bg-left;\n  padding-left: 3rem;\n}\n\n.header__button {\n  @include btn;\n  background-color: #fefefe;\n  padding: .5rem 2rem;\n  color: $pink;\n}","scss\\users.scss":"@import \"./variables.scss\";\n@import \"./mixin.scss\";\n\n.users__container {\n  background-color: $very-pale-blue;\n  padding-block: 50%;\n  text-align: center;\n}\n\n.users__header {\n  padding: 0;\n  margin-top: 20%;\n}\n\n.users__heading {\n  font-size: 2rem;\n  line-height: 4rem;\n  font-weight: bolder;\n}\n\n.users__image {\n  width: 80%;\n  height: 80%;\n  margin: auto;\n}","scss\\community.scss":"@import \"mixin.scss\";\n@import \"variables.scss\";\n\n.community {\n  padding-top: 80%;\n  padding-bottom: 50%;\n  background-repeat: no-repeat;\n  background-size: contain;\n  background-image: url('../images/screen-mockups.svg');\n  background-position: top center;\n  \n}\n\n.community__contianer {\n  text-align: center;\n}\n\n.community__heading {\n  font-size: 30px;\n  font-weight: bolder;\n  display: none;\n}\n\n.communities__big-text {\n  font-size: 6rem;\n  line-height: 10rem;\n  padding-top: 2rem;\n}\n\n.communities__big-text.members {\n  background-image: url('../images/icon-communities.svg');\n  @include bg-left;\n  background-position: top;\n}\n\n","scss\\build-community.scss":"@import \"./variables.scss\";\n@import \"./mixin.scss\";\n\n\n.build-community__container {\n  text-align: center;\n  padding-block: 50%;\n}\n\n.build-commnunity__heading {\n  text-transform: capitalize;\n  font-size: 30px;\n  font-weight: bolder;\n  line-height: 70px;\n}\n\n.build-community__button {\n  @include btn;\n  background-color: $pink;\n  padding: .7rem 2.5rem;\n  color: #fefefe;\n  box-shadow: 0px 12px 7px 1px #eeee;\n}","scss\\footer.scss":"@import \"./variables.scss\";\n\nfooter {\n  color: #fefefe;\n  padding-top: 3rem;\n  padding-bottom: 4rem;\n  background-image: url('../images/bg-footer-top-mobile.svg');\n  background-repeat: no-repeat;\n  background-position: top center;\n  background-size: 100% 6%;\n}\n\nfooter a {\n  color: #fefefe;\n  text-decoration: none;\n}\n\n.footer__container {\n  background-color: $very-dark-cyan;\n}\n\n.footer__container * {\n  margin: 0;\n}\n\n.newsletter__heading {\n  text-transform: uppercase;\n  padding-bottom: 1.5rem;\n}\n\n.newsletter__paragraph {\n  margin-block: 1rem;\n}\n\n.newsletter__form{\n  display: flex;\n  flex-direction: column;\n  align-items: flex-end;\n  padding-bottom: 5rem;\n}\n\n.newsletter__form > input{\n  background-color: #fefefe;\n  padding: .5rem;\n  display: block;\n  width: 98%;\n  border-radius: 7px;\n}\n\n.btn.newsletter__button.btn_submit {\n  @include btn;\n  padding: .5rem 1.3rem;\n  border-radius: 5px;\n  color: #fefefe;\n  margin-top: 1rem;\n  background-color: $pink;\n}\n\n.features__heading {\n  @include bg-left;\n  background-image: url('../images/icon-messages-white.svg');\n  padding-left: 3rem;\n  margin: 1rem 0;\n  font-weight: bolder;\n}\n\t\t\t\t\t\t\n.features__paragraph {\n  font-size: 16px;\n}\n\n.contact__phone, .contact__email {\n  @include bg-left;\n  padding-left: 2.5rem;\n  margin: 1rem 0;\n}\n.contact__phone {\n  background-image: url('../images/icon-phone.svg');\n}\n\t\t\t\t\t\t\n.contact__email {\n  background-image: url('../images/icon-email.svg');\n}\n\t\t\t\t\t\t\n.media__container {\n  padding: 0;\n}\n\n.anchor {\n  text-decoration: none;\n  @include bg-left;\n  background-position: center;\n}"},"lineCount":null}},"error":null,"hash":"bbc4986eaf129944a4402a5b2aa6ec98","cacheData":{"env":{}}}